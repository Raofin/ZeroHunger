
@{
    ViewBag.Title = "CollectOrders";
}

<h2>CollectOrders</h2>

<div>
    <table class="table table-bordered">
        <tr>
            <th>Id</th>
            <th>Request Id</th>
            <th>Description</th>
            <th>Restaurant Name</th>
            <th>Action</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Request_Id</td>
                <td>Description</td>
                <td>Res Name</td>
                <td><a href="/Employees/Collect/@item.Id">Collect</a></td>
            </tr>
        }
    </table>
</div>